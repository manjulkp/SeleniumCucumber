package pages.locators;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.How;

public class TelevisionBestBuyLocator {
	@FindBy(how=How.XPATH,using="//span[contains(text(),'20-28\"')]")
	public static WebElement screenSize;
	
	
	@FindBy(how=How.XPATH,using="//span[contains(text(),'LCD')]")
	public static WebElement screenType;

}
